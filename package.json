{
  "type": "module",
  "version": "0.1",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "build": "npx vite-ssg build",
    "dev": "vite --open",
    "lint": "eslint .",
    "preview": "vite preview",
